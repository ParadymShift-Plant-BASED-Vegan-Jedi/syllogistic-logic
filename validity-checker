$let[bc;$awaitButton[Does your argument have at least one premise?{button:Yes:GREEN::y}{button:No:RED::n};;30s;y;n]]
$if[bc==n]
    $let[$awaitmenu[What is the form of your conclusion?
    {title: Conclusion Form}
    {menu:
    {id=cf}
    {placeholder=Select}
    {min=1}
    {max=1}
    {label=all A is B}
    {value=1}
    {label=no A is B}
    {value=2}
    {label=some A is B}
    {value=3}
    {label=some A is not B}
    {value=4}
    {label=x is A}
    {value=5}
    {label=x is not A}
    {value=6}
    {label=x is y}
    {value=7}
    {label=x is not y}
    {value=8}
    }
    ;;30s;p1]
    $if[$get[mc]==1]
        $sendMessage[Your conclusion form is: all A is B.]
        $let[cf;1]
        $elseIf[$get[mc]==2]
            $sendMessage[Your conclusion forn is: no A is B.]
            $let[cf;2]
        $endelseIf
        $elseIf[$get[mc]==3]
            $sendMessage[Your conclusion form is: some A is B.]
            $let[cf;3]
        $endelseIf
        $elseIf[$get[mc]==4]
            $sendMessage[Your conclusion form is: some A is not B.]
            $let[cf;4]
        $endelseIf
        $elseIf[$get[mc]==5]
            $sendMessage[Your conclusion form is: x is A.]
            $let[cf;5]
        $endelseIf
        $elseIf[$get[mc]==6]
            $sendMessage[Your conclusion form is: x is not A.]
            $let[cf;6]
        $endelseIf
        $elseIf[$get[mc]==7]
            $sendMessage[Your conclusion form is: x is y.]
            $let[cf;7]
        $endelseIf
        $elseIf[$get[mc]==8]
            $sendMessage[Your conclusion form is: x is not y.]
            $let[cf;8]
        $endelseIf
        $elseIf[$get[mc]==undefined]
            $sendMessage[Error. Please make a selection next time!]
        $endelseIf
        $else
        $sendMessage[Unknown error. Please review code.]
    $endIf
    $let[ct1;$awaitMessage[What letter represents the first term of your conclusion? (Please enter a SINGLE letter ONLY; also note that the correct case :LEFT:whether upper or lower:RIGHT: will be automatically applied).;$authorID;20s;no]]
    $onlyIf[$charCount[$get[ct1]]==1;Please only include a single letter next time.]
    $let[ct2;$awaitMessage[What letter represents the second term of your conclusion? (Please enter a SINGLE letter ONLY; also note that the correct case :LEFT:whether upper or lower:RIGHT: will be automatically applied).;$authorID;20s;no]]
    $onlyIf[$charCount[$get[ct2]]==1;Please only include a single letter next time.]
    $if[$get[cf]<5]
        $let[ct1;$toUppercase[$get[ct1]]]
        $let[ct2;$toUppercase[$get[ct2]]]
        $elseIf[$get[cf]>4]
            $let[ct1;$toUppercase[$get[ct1]]]
            $if[$get[cf]==5||$get[cf]==6]
                $let[ct2;$toUppercase[$get[ct2]]]
                $elseIf[$get[cf]==7||$get[cf]==8]
                    $let[ct2;$toLowercase[$get[ct2]]]
                $endelseif
            $endIf
        $endelseif
    $endIf
    $sendMessage[Your final argument is:
âˆ´ $if[$get[cf]==1]all $get[ct1] is $get[ct2]$elseIf[$get[cf]==2]no $get[ct1] is $get[ct2]$endelseif$elseif[$get[cf]==3]some $get[ct1] is [$get[ct2]$endelseif$elseIf[$get[cf==4]some $get[ct1] is not $get[ct2]$endelseif$elseIf[$get[cf]==5]$get[ct1] is $get[ct2]$endelseif$elseIf[$get[cf]==6]$get[ct1] is not $get[ct2]$endelseif$elseIf[$get[cf]==7]$get[ct1] is $get[ct2]$endelseif$elseIf[$get[cf]==8]$get[ct1] is not $get[ct2]$endelseif$endIf]
    $if[
    
$let[mc;$awaitMenu[Let's create your first premise. What is the first operator of premise 1?
{title: Premise 1}
{menu:
{id=p1o1}
{placeholder=Select}
{min=1}
{max=1}
{label=all}
{value=all}
{label=no}
{value=no}
{label=some}
{value=some}
{label=None}
{value=none}
}
;;30s;p1o1]
$

