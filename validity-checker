            $let[p1t1;$toUppercase[$get[p1t1]]]
            $let[p1t2;$toUppercase[$get[p1t2]]]
            $elseIf[$get[p1f]>4]
                $let[p1t1;$toUppercase[$get[p1t1]]]
                $if[$get[p1f]==5||$get[p1f]==6]
                    $let[p1t2;$toUppercase[$get[p1t2]]]
                    $elseIf[$get[p1f]==7||$get[p1f]==8]
                        $let[p1t2;$toLowercase[$get[p1t2]]]
                    $endelseif
                $endIf
            $endelseif
        $endIf
        $sendMessage[Your first premise is: $if[$get[p1f]==1]all $get[p1t1] is $get[p1t2]$elseIf[$get[p1f]==2]no $get[p1t1] is $get[p1t2]$endelseif$elseif[$get[p1f]==3]some $get[p1t1] is [$get[p1t2]$endelseif$elseIf[$get[p1f==4]some $get[p1t1] is not $get[p1t2]$endelseif$elseIf[$get[[p1f]==5]$get[p1t1] is $get[p1t2]$endelseif$elseIf[$get[p1f]==6]$get[p1t1] is not $get[p1t2]$endelseif$elseIf[$get[p1f]==7]$get[p1t1] is $get[p1t2]$endelseif$elseIf[$get[p1f]==8]$get[p1t1] is not $get[p1t2]$endelseif$endIf]
        $let[bc;$awaitButton[Does your argument have a second premise?{button:Yes:GREEN::y}{button:No:RED::n};;20s;y;n]]
        $if[$get[bc]==n]
            $let[cf;$awaitmenu[What is the form of your conclusion?
