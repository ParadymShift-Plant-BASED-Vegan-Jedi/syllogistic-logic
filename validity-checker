$let[bc;$awaitButton[Does your argument have at least one premise?{button:Yes:GREEN::y}{button:No:RED::n};;30s;y;n]]
$if[bc==n]
    $let[cf;$awaitmenu[What is the form of your conclusion?
    {title: Conclusion Form}
    {menu:
    {id=cform}
    {placeholder=Select}
    {min=1}
    {max=1}
    {label=all A is B}
    {value=1}
    {label=no A is B}
    {value=2}
    {label=some A is B}
    {value=3}
    {label=some A is not B}
    {value=4}
    {label=x is A}
    {value=5}
    {label=x is not A}
    {value=6}
    {label=x is y}
    {value=7}
    {label=x is not y}
    {value=8}
    }
    ;;30s;p1]
    $if[$get[cf]==1]
        $sendMessage[Your conclusion form is: all A is B.]
        $elseIf[$get[cf]==2]
            $sendMessage[Your conclusion form is: no A is B.]
        $endelseIf
        $elseIf[$get[cf]==3]
            $sendMessage[Your conclusion form is: some A is B.]
        $endelseIf
        $elseIf[$get[cf]==4]
            $sendMessage[Your conclusion form is: some A is not B.]
        $endelseIf
        $elseIf[$get[cf]==5]
            $sendMessage[Your conclusion form is: x is A.]
        $endelseIf
        $elseIf[$get[cf]==6]
            $sendMessage[Your conclusion form is: x is not A.]
        $endelseIf
        $elseIf[$get[cf]==7]
            $sendMessage[Your conclusion form is: x is y.]
        $endelseIf
        $elseIf[$get[cf]==8]
            $sendMessage[Your conclusion form is: x is not y.]
        $endelseIf
        $elseIf[$get[cf]==undefined]
            $stop[Error. Please make a selection next time!]
        $endelseIf
        $else
        $stop[Unknown error. Please review code.]
    $endIf
    $let[ct1;$awaitMessage[What letter represents the first term of your conclusion? (Please enter a SINGLE letter ONLY; also note that the correct case :LEFT:whether upper or lower:RIGHT: will be automatically applied).;$authorID;20s;no]]
    $onlyIf[$charCount[$get[ct1]]==1;Please only include a single letter next time.]
    $let[ct2;$awaitMessage[What letter represents the second term of your conclusion? (Please enter a SINGLE letter ONLY; also note that the correct case :LEFT:whether upper or lower:RIGHT: will be automatically applied).;$authorID;20s;no]]
    $onlyIf[$charCount[$get[ct2]]==1;Please only include a single letter next time.]
    $if[$get[cf]<5]
        $let[ct1;$toUppercase[$get[ct1]]]
        $let[ct2;$toUppercase[$get[ct2]]]
        $elseIf[$get[cf]>4]
            $let[ct1;$toUppercase[$get[ct1]]]
            $if[$get[cf]==5||$get[cf]==6]
                $let[ct2;$toUppercase[$get[ct2]]]
                $elseIf[$get[cf]==7||$get[cf]==8]
                    $let[ct2;$toLowercase[$get[ct2]]]
                $endelseif
            $endIf
        $endelseif
    $endIf
    $sendMessage[Your final argument is:
∴ $if[$get[cf]==1]all $get[ct1] is $get[ct2]$elseIf[$get[cf]==2]no $get[ct1] is $get[ct2]$endelseif$elseif[$get[cf]==3]some $get[ct1] is [$get[ct2]$endelseif$elseIf[$get[cf==4]some $get[ct1] is not $get[ct2]$endelseif$elseIf[$get[cf]==5]$get[ct1] is $get[ct2]$endelseif$elseIf[$get[cf]==6]$get[ct1] is not $get[ct2]$endelseif$elseIf[$get[cf]==7]$get[ct1] is $get[ct2]$endelseif$elseIf[$get[cf]==8]$get[ct1] is not $get[ct2]$endelseif$endIf]
    $if[$get[cf]==1||$get[cf]==3||$get[cf]==5||$get[cf]==7]
        $let[rc;1]
        $elseIf[$get[cf]==2||$get[cf]==4||$get[cf]==6||$get[cf]==8]
            $let[rc;0]
        $endelseif
    $endIf
    $if[$get[cf]==1]
        $if[$get[ct1]==$get[ct2]]
            $let[flc;true]
            $else
            $let[flc;false]
        $endIf
        $elseIf[$get[cf]==2||$get[cf]==4||$get[cf]==6]
            $let[flc;false]
        $endelseif
        $elseIf[$get[cf]==3]
            $if[$get[ct1]==$get[ct2]]
                $let[flc;false]
                $else
                $let[flc;true]
            $endIf
        $endelseif
        $elseIf[$get[cf]==5||$get[cf]==7||$get[cf]==8]
            $let[flc;true]
        $endelseif
    $endIf
    $if[$get[flc]==true&&$get[rc]==1]
        $let[result;valid]
        $else
        $let[result;invalid]
    $endIf
    $sendMessage[Your argument is: $get[result].]  
