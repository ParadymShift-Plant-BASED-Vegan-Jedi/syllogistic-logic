"Q":"17",
"R":"18",
"S":"19",
"T":"20",
"U":"21",
"V":"22",
"W":"23",
"X":"24",
"Y":"25",
"Z":"26",
"a":"1s",
"b":"2s",
"c":"3s",
"d":"4s",
"e":"5s",
"f":"6s",
"g":"7s",
"h":"8s",
"i":"9s",
"j":"10s",
"k":"11s",
"l":"12s",
"m":"13s",
"n":"14s",
"o":"15s",
"p":"16s",
"q":"17s",
"r":"18s",
"s":"19s",
"t":"20s",
"u":"21s",
"v":"22s",
"w":"23s",
"x":"24s",
"y":"25s",
"z":"26s"
}
]
$let[bc;$awaitButton[Does your argument have at least one premise?{button:Yes:GREEN::y}{button:No:RED::n};;30s;y;n]]
$if[bc==n]
    $let[cf;$awaitmenu[What is the form of your conclusion?
    {title: Conclusion Form}
    {menu:
    {id=cform}
    {placeholder=Select}
    {min=1}
    {max=1}
    {label=all A is B}
    {value=1}
    {label=no A is B}
    {value=2}
    {label=some A is B}
    {value=3}
    {label=some A is not B}
    {value=4}
    {label=x is A}
    {value=5}
    {label=x is not A}
    {value=6}
    {label=x is y}
    {value=7}
    {label=x is not y}
    {value=8}
    }
    ;;30s;p1]
    $if[$get[cf]==1]
        $sendMessage[Your conclusion form is: all A is B.]
        $elseIf[$get[cf]==2]
            $sendMessage[Your conclusion form is: no A is B.]
        $endelseIf
        $elseIf[$get[cf]==3]
            $sendMessage[Your conclusion form is: some A is B.]
        $endelseIf
        $elseIf[$get[cf]==4]
            $sendMessage[Your conclusion form is: some A is not B.]
        $endelseIf
        $elseIf[$get[cf]==5]
            $sendMessage[Your conclusion form is: x is A.]
        $endelseIf
        $elseIf[$get[cf]==6]
            $sendMessage[Your conclusion form is: x is not A.]
        $endelseIf
        $elseIf[$get[cf]==7]
            $sendMessage[Your conclusion form is: x is y.]
        $endelseIf
        $elseIf[$get[cf]==8]
            $sendMessage[Your conclusion form is: x is not y.]
        $endelseIf
        $elseIf[$get[cf]==undefined]
            $stop[Error. Please make a selection next time!]
        $endelseIf
        $else
        $stop[Unknown error. Please review code.]
    $endIf
    $let[ct1;$awaitMessage[What letter represents the first term of your conclusion? (Please enter a SINGLE letter ONLY; also note that the correct case :LEFT:whether upper or lower:RIGHT: will be automatically applied).;$authorID;20s;no]]
    $onlyIf[$charCount[$get[ct1]]==1;Error. Please only include a single letter next time.]
    $onlyIf[$isNumber[$get[ct1]]==false;Error. Please only include a single letter next time!]
    $let[ct2;$awaitMessage[What letter represents the second term of your conclusion? (Please enter a SINGLE letter ONLY; also note that the correct case :LEFT:whether upper or lower:RIGHT: will be automatically applied).;$authorID;20s;no]]
    $onlyIf[$charCount[$get[ct2]]==1;Error. Please only include a single letter next time.]
    $onlyIf[$isNumber[$get[ct2]]==false;Error. Please only include a single letter next time!]
    $if[$get[cf]<5]
        $let[ct1;$toUppercase[$get[ct1]]]
        $let[ct2;$toUppercase[$get[ct2]]]
        $elseIf[$get[cf]>4]
            $let[ct1;$toLowercase[$get[ct1]]]
            $if[$get[cf]==5||$get[cf]==6]
                $let[ct2;$toUppercase[$get[ct2]]]
                $elseIf[$get[cf]==7||$get[cf]==8]
                    $let[ct2;$toLowercase[$get[ct2]]]
                $endelseif
            $endIf
        $endelseif
    $endIf
    $sendMessage[Your final argument is:
∴ $if[$get[cf]==1]all $get[ct1] is $get[ct2]$elseIf[$get[cf]==2]no $get[ct1] is $get[ct2]$endelseif$elseif[$get[cf]==3]some $get[ct1] is [$get[ct2]$endelseif$elseIf[$get[cf==4]some $get[ct1] is not $get[ct2]$endelseif$elseIf[$get[cf]==5]$get[ct1] is $get[ct2]$endelseif$elseIf[$get[cf]==6]$get[ct1] is not $get[ct2]$endelseif$elseIf[$get[cf]==7]$get[ct1] is $get[ct2]$endelseif$elseIf[$get[cf]==8]$get[ct1] is not $get[ct2]$endelseif$endIf]
    $if[$get[cf]==1||$get[cf]==3||$get[cf]==5||$get[cf]==7]
        $let[rc;1]
        $elseIf[$get[cf]==2||$get[cf]==4||$get[cf]==6||$get[cf]==8]
            $let[rc;0]
        $endelseif
    $endIf
    $if[$get[cf]==1]
        $if[$get[ct1]==$get[ct2]]
            $let[flc;true]
            $else
            $let[flc;false]
        $endIf
        $elseIf[$get[cf]==2||$get[cf]==4||$get[cf]==6]
            $let[flc;false]
        $endelseif
        $elseIf[$get[cf]==3]
            $if[$get[ct1]==$get[ct2]]
                $let[flc;false]
                $else
                $let[flc;true]
            $endIf
        $endelseif
        $elseIf[$get[cf]==5||$get[cf]==7||$get[cf]==8]
            $let[flc;true]
        $endelseif
    $endIf
    $if[$get[flc]==true&&$get[rc]==1]
        $let[result;valid]
        $else
        $let[result;invalid]
    $endIf
    $sendMessage[Your argument is: $get[result].]  
    $elseIf[$get[bc]==y]
        $let[p1f;$awaitMenu[Let's create your first premise. What is the form of premise 1?
        {title: Premise 1 Form}
        {menu:
        {id=p1form}
        {placeholder=Select}
        {min=1}
        {max=1}
        {label=all A is B}
        {value=1}
        {label=no A is B}
        {value=2}
        {label=some A is B}
        {value=3}
        {label=some A is not B}
        {value=4}
        {label=x is A}
        {value=5}
        {label=x is not A}
        {value=6}
        {label=x is y}
        {value=7}
        {label=x is not y}
        {value=8}
        }
        ;;30s;p1form]
        $if[$get[p1f]==1]
            $sendMessage[Your premise 1 form is: all A is B.]
            $elseIf[$get[p1f]==2]
                $sendMessage[Your premise 1 form is: no A is B.]
            $endelseIf
            $elseIf[$get[[p1f]==3]
                $sendMessage[Your premise 1 form is: some A is B.]
            $endelseIf
            $elseIf[$get[p1f]==4]
                $sendMessage[Your premise 1 form is: some A is not B.]
            $endelseIf
            $elseIf[$get[p1f]==5]
                $sendMessage[Your premise 1 form is: x is A.]
            $endelseIf
            $elseIf[$get[p1f]==6]
                $sendMessage[Your premise 1 form is: x is not A.]
            $endelseIf
            $elseIf[$get[p1f]==7]
                $sendMessage[Your conclusion form is: x is y.]
            $endelseIf
            $elseIf[$get[p1f]==8]
                $sendMessage[Your premise 1 form is: x is not y.]
            $endelseIf
            $elseIf[$get[p1f]==undefined]
                $stop[Error. Please make a selection next time!]
            $endelseIf
            $else
            $stop[Unknown error. Please review code.]
        $endIf
        $let[p1t1;$awaitMessage[What letter represents the first term of your first premise? (Please enter a SINGLE letter ONLY; also note that the correct case :LEFT:whether upper or lower:RIGHT: will be automatically applied).;$authorID;20s;no]]
        $onlyIf[$charCount[$get[p1t1]]==1;Error. Please only include a single letter next time.]
        $onlyIf[$isNumber[$get[p1t1]]==false;Error. Please only include a single letter next time!]
        $let[p1t2;$awaitMessage[What letter represents the second term of your first premise? (Please enter a SINGLE letter ONLY; also note that the correct case :LEFT:whether upper or lower:RIGHT: will be automatically applied).;$authorID;20s;no]]
        $onlyIf[$charCount[$get[p1t2]]==1;Error. Please only include a single letter next time.]
        $onlyIf[$isNumber[$get[p1t2]]==false;Error. Please only include a single letter next time!]
        $if[$get[p1f]<5]
            $let[p1t1;$toUppercase[$get[p1t1]]]
            $let[p1t2;$toUppercase[$get[p1t2]]]
            $elseIf[$get[p1f]>4]
                $let[p1t1;$toLowercase[$get[p1t1]]]
                $if[$get[p1f]==5||$get[p1f]==6]
                    $let[p1t2;$toUppercase[$get[p1t2]]]
                    $elseIf[$get[p1f]==7||$get[p1f]==8]
                        $let[p1t2;$toLowercase[$get[p1t2]]]
                    $endelseif
                $endIf
            $endelseif
        $endIf
        $sendMessage[Your first premise is: $if[$get[p1f]==1]all $get[p1t1] is $get[p1t2]$elseIf[$get[p1f]==2]no $get[p1t1] is $get[p1t2]$endelseif$elseif[$get[p1f]==3]some $get[p1t1] is [$get[p1t2]$endelseif$elseIf[$get[p1f==4]some $get[p1t1] is not $get[p1t2]$endelseif$elseIf[$get[[p1f]==5]$get[p1t1] is $get[p1t2]$endelseif$elseIf[$get[p1f]==6]$get[p1t1] is not $get[p1t2]$endelseif$elseIf[$get[p1f]==7]$get[p1t1] is $get[p1t2]$endelseif$elseIf[$get[p1f]==8]$get[p1t1] is not $get[p1t2]$endelseif$endIf]
        $let[bc;$awaitButton[Does your argument have a second premise?{button:Yes:GREEN::y}{button:No:RED::n};;20s;y;n]]
        $if[$get[bc]==n]
            $let[cf;$awaitmenu[What is the form of your conclusion?
            {title: Conclusion Form}
            {menu:
            {id=cform}
            {placeholder=Select}
            {min=1}
            {max=1}
            {label=all A is B}
            {value=1}
            {label=no A is B}
            {value=2}
            {label=some A is B}
            {value=3}
            {label=some A is not B}
            {value=4}
            {label=x is A}
            {value=5}
            {label=x is not A}
            {value=6}
            {label=x is y}
            {value=7}
            {label=x is not y}
            {value=8}
            }
            ;;30s;cform]
            $if[$get[cf]==1]
                $sendMessage[Your conclusion form is: all A is B.]
                $elseIf[$get[cf]==2]
                    $sendMessage[Your conclusion form is: no A is B.]
                $endelseIf
                $elseIf[$get[cf]==3]
                    $sendMessage[Your conclusion form is: some A is B.]
                $endelseIf
                $elseIf[$get[cf]==4]
                    $sendMessage[Your conclusion form is: some A is not B.]
                $endelseIf
                $elseIf[$get[cf]==5]
                    $sendMessage[Your conclusion form is: x is A.]
                $endelseIf
                $elseIf[$get[cf]==6]
                    $sendMessage[Your conclusion form is: x is not A.]
                $endelseIf
                $elseIf[$get[cf]==7]
                    $sendMessage[Your conclusion form is: x is y.]
                $endelseIf
                $elseIf[$get[cf]==8]
                    $sendMessage[Your conclusion form is: x is not y.]
                $endelseIf
                $elseIf[$get[cf]==undefined]
                    $stop[Error. Please make a selection next time!]
                $endelseIf
                $else
                $stop[Unknown error. Please review code.]
            $endIf
            $let[ct1;$awaitMessage[What letter represents the first term of your conclusion? (Please enter a SINGLE letter ONLY; also note that the correct case :LEFT:whether upper or lower:RIGHT: will be automatically applied).;$authorID;20s;no]]
            $onlyIf[$charCount[$get[ct1]]==1;Error. Please only include a single letter next time.]
            $onlyIf[$isNumber[$get[ct1]]==false;Error. Please only include a single letter next time!]
            $let[ct2;$awaitMessage[What letter represents the second term of your conclusion? (Please enter a SINGLE letter ONLY; also note that the correct case :LEFT:whether upper or lower:RIGHT: will be automatically applied).;$authorID;20s;no]]
            $onlyIf[$charCount[$get[ct2]]==1;Error. Please only include a single letter next time.]
            $onlyIf[$isNumber[$get[ct2]]==false;Error. Please only include a single letter next time!]
            $if[$get[cf]<5]
                $let[ct1;$toUppercase[$get[ct1]]]
                $let[ct2;$toUppercase[$get[ct2]]]
                $elseIf[$get[cf]>4]
                    $let[ct1;$toLowercase[$get[ct1]]]
                    $if[$get[cf]==5||$get[cf]==6]
                        $let[ct2;$toUppercase[$get[ct2]]]
                        $elseIf[$get[cf]==7||$get[cf]==8]
                            $let[ct2;$toLowercase[$get[ct2]]]
                        $endelseif
                    $endIf
                $endelseif
            $endIf
            $sendMessage[Your final argument is: 
P1: $if[[$get[p1f]==1]all $get[p1t1] is $get[p1t2]$elseIf[$get[p1f]==2]no $get[p1t1] is $get[p1t2]$endelseif$elseif[$get[p1f]==3]some $get[p1t1] is [$get[p1t2]$endelseif$elseIf[$get[p1f==4]some $get[p1t1] is not $get[p1t2]$endelseif$elseIf[$get[[p1f]==5]$get[p1t1] is $get[p1t2]$endelseif$elseIf[$get[p1f]==6]$get[p1t1] is not $get[p1t2]$endelseif$elseIf[$get[p1f]==7]$get[p1t1] is $get[p1t2]$endelseif$elseIf[$get[p1f]==8]$get[p1t1] is not $get[p1t2]$endelseif$endIf
∴ $if[$get[cf]==1]all $get[ct1] is $get[ct2]$elseIf[$get[cf]==2]no $get[ct1] is $get[ct2]$endelseif$elseif[$get[cf]==3]some $get[ct1] is [$get[ct2]$endelseif$elseIf[$get[cf==4]some $get[ct1] is not $get[ct2]$endelseif$elseIf[$get[cf]==5]$get[ct1] is $get[ct2]$endelseif$elseIf[$get[cf]==6]$get[ct1] is not $get[ct2]$endelseif$elseIf[$get[cf]==7]$get[ct1] is $get[ct2]$endelseif$elseIf[$get[cf]==8]$get[ct1] is not $get[ct2]$endelseif$endIf]
            $if[$get[p1f]==1||$get[p1f]==3||$get[p1f]==5||$get[p1f]==7]
                $let[rc;0]
                $elseIf[$get[p1f]==2||$get[p1f]==4||$get[p1f]==6||$get[p1f]==8]
                    $let[rc;1]
                $endelseif
            $endIf
            $if[$get[cf]==1||$get[cf]==3||$get[cf]==5||$get[cf]==7]
                $let[rc;$sum[$get[rc];1]]
            $endIf
            $onlyIf[$get[rc]==1;Your argument is: invalid.]
            $if[$get[p1f]==1]
                $let[$objectGet[$get[p1t1]]c;1]
                $elseIf[$get[p1f]==2]
                    $let[$objectGet[$get[p1t1]]c;1]
                    $let[$objectGet[$get[p1t2]]c;1]
                $endelseif
                $elseIf[$get[p1f]==4||$get[p1f]==6]
                    $let[$objectGet[$get[p1t2]]c;1]
                $endelseif
            $endIf
            $if[$get[cf]==1||$get[cf]==5]
                $onlyIf[$get[$objectGet[$get[ct2]]c]==undefined;Your argument is: invalid.]
                $let[$objectGet[$get[ct2]]c;1]
                $elseIf[$get[cf==3]
                    $onlyIf[$get[$objectGet[$get[ct1]]c]==undefined;Your argument is: invalid.]
                    $let[$objectGet[$get[ct1]]c;1]
                    $onlyIf[$get[$objectGet[$get[ct2]]c]==undefined;Your argument is: invalid.]
                    $let[$objectGet[$get[ct2]]c;1]
                $endelseif
                $elseIf[$get[cf==4]
                    $onlyIf[$get[$objectGet[$get[ct1]]c]==undefined;Your argument is: invalid.]
                    $let[$objectGet[$get[ct1]c;1]
                $endelseif
             $endIf
             $if[$get[p1f]==1||$get[p1f]==2||$get[p1f]==3||$get[p1f]==4]
                 $onlyIf[$get[$objectGet[$get[p1t1]]c]==1&&$get[$objectGet[p1t2]c]==1;Your argument is: invalid.]
                 $elseIf[$get[p1f]==5||$get[p1f]==6]
                     $onlyIf[$get[$objectGet[$get[p1t2]]c]==1;Your argument is: invalid.]
                 $endelseif
             $endIf
             $if[$get[cf]==1||$get[cf]==2||$get[cf]==3||$get[cf]==4]
                 $onlyIf[$get[$objectGet[$get[ct1]]c]==1&&$get[$objectGet[ct2]c]==1;Your argument is: invalid.]
                 $elseIf[$get[cf]==5||$get[cf]==6]
                     $onlyIf[$get[$objectGet[$get[ct2]]c]==1;Your argument is: invalid.]
                 $endelseif
             $endIf
             $sendMessage[Your argument is: valid.]
             $elseIf[$get[bc]==y]
                 $let[p2f;$awaitMenu[Let's create your second premise. What is the form of premise 2?
                 {title: Premise 2 Form}
                 {menu:
                 {id=p2form}
                 {placeholder=Select}
                 {min=1}
                 {max=1}
                 {label=all A is B}
                 {value=1}
                 {label=no A is B}
                 {value=2}
                 {label=some A is B}
                 {value=3}
                 {label=some A is not B}
                 {value=4}
                 {label=x is A}
                 {value=5}
                 {label=x is not A}
                 {value=6}
                 {label=x is y}
                 {value=7}
                 {label=x is not y}
                 {value=8}
                 }
                 ;;30s;p2form]
                 $if[$get[p2f]==1]
                     $sendMessage[Your premise 2 form is: all A is B.]
                     $elseIf[$get[p2f]==2]
                         $sendMessage[Your premise 2 form is: no A is B.]
                     $endelseIf
                     $elseIf[$get[[p2f]==3]
                         $sendMessage[Your premise 2 form is: some A is B.]
                     $endelseIf
                     $elseIf[$get[p2f]==4]
                         $sendMessage[Your premise 2 form is: some A is not B.]
                     $endelseIf
                     $elseIf[$get[p2f]==5]
                         $sendMessage[Your premise 2 form is: x is A.]
                     $endelseIf
                     $elseIf[$get[p2f]==6]
                         $sendMessage[Your premise 2 form is: x is not A.]
                     $endelseIf
                     $elseIf[$get[p2f]==7]
                         $sendMessage[Your premise 2 form is: x is y.]
                     $endelseIf
                     $elseIf[$get[p2f]==8]
